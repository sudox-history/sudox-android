<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="NavigationBar">
        <attr name="backButtonIsVisible" format="boolean" />
        <attr name="nextButtonIsVisible" format="boolean" />
        <attr name="sudoxTagIsVisible" format="boolean" />
        <attr name="someFeatureButtonIsVisible" format="boolean" />
        <attr name="backButtonText" format="string" />
        <attr name="nextButtonText" format="string" />
        <attr name="someFeatureButtonText" format="string" />
    </declare-styleable>

    <declare-styleable name="SudoxToolbar">
        <attr name="featureButtonText" format="string" />
    </declare-styleable>

    <declare-styleable name="PrecomputedTextView">
        <attr name="text" format="string" />
    </declare-styleable>

    <declare-styleable name="SheetButton">
        <attr name="buttonIcon" format="integer" />
        <attr name="buttonText" format="string" />
    </declare-styleable>
</resources>