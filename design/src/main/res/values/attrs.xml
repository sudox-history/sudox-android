<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="EditTextLayout">
        <attr name="labelStyle" />
        <attr name="errorTextColor" />
    </declare-styleable>

    <declare-styleable name="NavigationBar">
        <attr name="titleStyle" />
        <attr name="buttonsStyle" />
    </declare-styleable>

    <declare-styleable name="NavigationBarButton">
        <attr name="iconTintColor" />
        <attr name="iconTextMargin" />
    </declare-styleable>

    <attr name="labelStyle" format="reference" />
    <attr name="titleStyle" format="reference" />
    <attr name="buttonsStyle" format="reference" />
    <attr name="editTextLayoutStyle" format="reference" />
    <attr name="navigationBarStyle" format="reference" />

    <attr name="iconTintColor" format="color" />
    <attr name="iconTextMargin" format="dimension" />
    <attr name="errorTextColor" format="color" />
</resources>