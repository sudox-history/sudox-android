<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PhoneEditText">
        <attr name="autofillMyNumber" format="boolean" />
        <attr name="separatorDrawable" format="reference" />
        <attr name="separatorDrawableWidth" format="dimension" />
        <attr name="separatorDrawableHeight" format="dimension" />
        <attr name="separatorLeftMargin" format="dimension" />
        <attr name="separatorRightMargin" format="dimension" />
    </declare-styleable>

    <declare-styleable name="PhoneCountryCodeSelector">
        <attr name="flagMargin" />
        <attr name="flagHeight" />
        <attr name="flagWidth" />
    </declare-styleable>

    <declare-styleable name="CodeEditText">
        <attr name="digitsCount" format="integer" />
    </declare-styleable>

    <declare-styleable name="ApplicationBar">
        <attr name="titleTextAppearance" format="reference" />
    </declare-styleable>

    <declare-styleable name="ApplicationBarButton">
        <attr name="textSize" />
        <attr name="textTypeface" />
        <attr name="textColor" />

        <attr name="iconHeight" />
        <attr name="iconWidth" />
        <attr name="iconMargin" />
    </declare-styleable>

    <declare-styleable name="NicknameEditText">
        <attr name="tagTextColor" format="color" />
    </declare-styleable>

    <declare-styleable name="PercentRelativeLayout">
        <attr name="paddingTopPercent" format="float" />
        <attr name="paddingBottomPercent" format="float" />
        <attr name="paddingRightPercent" format="float" />
        <attr name="paddingLeftPercent" format="float" />
    </declare-styleable>

    <declare-styleable name="EditTextLayout">
        <attr name="errorTextAppearance" format="reference" />
        <attr name="errorTextTopMargin" format="dimension" />
        <attr name="errorTextLeftMargin" format="dimension" />
        <attr name="strokeColor" format="color" />
        <attr name="strokeWidth" format="dimension" />
    </declare-styleable>

    <declare-styleable name="SortedListView">
        <attr name="lettersRightPadding" format="dimension" />
        <attr name="lettersTopPadding" format="dimension" />
        <attr name="lettersMargin" format="dimension" />
        <attr name="lettersFont" format="reference" />
        <attr name="lettersTextSize" format="dimension" />
        <attr name="lettersColor" format="color" />
    </declare-styleable>

    <declare-styleable name="CountryItemView">
        <attr name="flagMargin" />
        <attr name="flagHeight" />
        <attr name="flagWidth" />
        <attr name="textAppearance" format="reference" />
    </declare-styleable>

    <declare-styleable name="NavigationBarButton">
        <attr name="defaultContentColor" format="color" />
        <attr name="focusedContentColor" format="color" />

        <attr name="titleFont" format="reference" />
        <attr name="titleTextSize" format="dimension" />

        <attr name="iconWidth" />
        <attr name="iconHeight" />
    </declare-styleable>

    <declare-styleable name="TabLayout">
        <attr name="indicatorColor" format="color" />
        <attr name="indicatorHeight" format="dimension" />
        <attr name="indicatorMargin" format="dimension" />
        <attr name="indicatorPaddingRight" format="dimension" />
        <attr name="indicatorPaddingLeft" format="dimension" />
    </declare-styleable>

    <declare-styleable name="TabLayoutButton">
        <attr name="activeTextColor" format="color" />
        <attr name="defaultTextColor" format="color" />

        <attr name="textSize" />
        <attr name="textTypeface" />
    </declare-styleable>

    <attr name="flagMargin" format="dimension" />
    <attr name="flagHeight" format="dimension" />
    <attr name="flagWidth" format="dimension" />

    <attr name="iconWidth" format="dimension" />
    <attr name="iconHeight" format="dimension" />
    <attr name="iconMargin" format="dimension" />

    <attr name="textSize" format="dimension" />
    <attr name="textTypeface" format="reference" />
    <attr name="textColor" format="color" />

    <attr name="countryItemViewStyle" format="reference" />
    <attr name="countryCodeSelectorStyle" format="reference" />
    <attr name="phoneEditTextStyle" format="reference" />
    <attr name="codeEditTextStyle" format="reference" />
    <attr name="nicknameEditTextStyle" format="reference" />
    <attr name="applicationBarStyle" format="reference" />
    <attr name="applicationBarButtonStyle" format="reference" />
    <attr name="editTextLayoutStyle" format="reference" />
    <attr name="sortedListViewStyle" format="reference" />
    <attr name="navigationBarStyle" format="reference" />
    <attr name="navigationBarButtonStyle" format="reference" />
    <attr name="tabLayoutStyle" format="reference" />
    <attr name="tabLayoutButtonStyle" format="reference" />
</resources>