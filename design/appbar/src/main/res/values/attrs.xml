<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AppBar">
        <attr name="buttonsStyle" format="reference" />
        <attr name="titleTextAppearance" format="reference" />
        <attr name="marginBetweenViewsAndButtons" format="dimension" />
        <attr name="marginBetweenViews" format="dimension" />
        <attr name="pseudoPaddingRight" format="dimension" />
        <attr name="pseudoPaddingLeft" format="dimension" />
    </declare-styleable>

    <declare-styleable name="CustomAppBarLayout">
        <attr name="strokeAnimationDuration" format="integer" />
        <attr name="strokeWidth" format="dimension" />
        <attr name="strokeColor" format="color" />
    </declare-styleable>

    <attr name="appBarStyle" format="reference" />
    <attr name="customAppBarLayoutStyle" format="reference" />
    <attr name="appBarSearchEditTextStyle" format="reference" />
</resources>